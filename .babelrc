{
    "presets": ["es2015", "stage-0"],
    "env": {
        "development": {
            "plugins": [
                "transform-runtime",
                "transform-flow-strip-types",
                "syntax-flow",
                "typecheck",
                "contracts"
            ]
        },
        "production": {
            "plugins": [
                "transform-runtime",
                "transform-flow-strip-types",
                "syntax-flow",
                ["contracts", { "strip": true }]
            ]
        }
    }
}
