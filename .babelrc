{
    "presets": ["es2015"],
    "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-function-bind"
     ],
    "env": {
        "development": {
            "plugins": [
                "contracts"
            ]
        },
        "production": {
            "plugins": [
                ["contracts", { "strip": true }]
            ]
        }
    }
}
